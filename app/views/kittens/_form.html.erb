<%= form_for @kitten do |f| %>
  <%= f.label :name, 'Kitten Name' %>
  <%= f.text_field :name %>

  <%= f.label :age, "Kitten's Age" %>
  <%= f.number_field :age %>

  <%= f.label :cuteness, "Kitten's Cuteness level" %>
  <%= f.number_field :cuteness %>

  <%= f.label :softness, "Kitten's Softness level" %>
  <%= f.number_field :softness %>

  <%= f.submit "Submit" %>

<% end %>